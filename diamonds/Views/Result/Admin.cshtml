@model IEnumerable<Diamonds.Models.Entities.Result>

@{
    ViewBag.Title = "Admin";
}

@Html.Partial("_NavigationBar", new Diamonds.Models.NavigationModel("Result", "Lista", "MODERATOR"))

<fieldset>
    <legend>Wyniki</legend>

    <table class="purple">
        <tr>
            <th>gospodarze</th>
            <th></th>
            <th></th>
            <th></th>
            <th>goście</th>
            <th></th>
        </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>@item.TeamHome.name</td>
        <td>@item.resultHome</td>
        <td>:</td>
        <td>@item.resultGuest</td>
        <td>@item.TeamGuest.name</td>
        <td>@Html.ActionLink("Edit", "Edit", new { id = item.id })</td>
    </tr>
    }

    </table>
</fieldset>