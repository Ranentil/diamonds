@{
    ViewBag.Title = "Dodawanie zdjęć";
}

@using (Html.BeginForm("AddPhotos", "Gallery", new { id = ViewBag.Company.id }, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="file" id="photos" name="photos" multiple="multiple" />
    <input type="submit" value="Wyślij" />
}